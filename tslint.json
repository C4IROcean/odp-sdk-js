{
	"extends": "tslint:latest",
	"rules": {
		"prefer-conditional-expression": false,
		"object-literal-key-quotes": [true, "as-needed"],
		"ordered-imports": [false],
		"object-literal-sort-keys": false,
		"variable-name": [false],
		"indent": [true, "tabs"],
		"semicolon": [true, "always", "strict-bound-class-methods"],
		"member-ordering": [
			true,
			{
				"order": "fields-first"
			}
		]
	}
}
