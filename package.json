{
	"name": "odp-sdk",
	"version": "0.7.1",
	"description": "Javascript client library for ODP SDK, built on top of Cognite SDK",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run format-code && npm run lint && tsc && npm test",
		"build:ci": "npm run lint && tsc --sourceMap false",
		"docs": "typedoc --options typedoc.js --tsconfig tsconfig.json ./source",
		"lint": "npm run eslint",
		"eslint": "eslint source/ tests/ integration_tests/ --ext .ts,.d.ts",
		"format-code": "prettier package.json index.ts \"source/**/*.ts\" --write",
		"test": "jest",
		"compile": "tsc"
	},
	"keywords": [
		"ODP",
		"sdk",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/C4IROcean/odp-sdk-js.git"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@cognite/sdk": "^2.33.0",
		"cd": "^0.3.3",
		"geolib": "^3.3.1",
		"lodash": "^4.17.20",
		"ndjson": "^1.5.0"
	},
	"devDependencies": {
		"@types/jest": "^24.9.1",
		"@types/node": "^13.13.23",
		"@typescript-eslint/eslint-plugin": "4.14.1",
		"@typescript-eslint/parser": "4.14.1",
		"eslint": "7.19.0",
		"eslint-config-prettier": "7.2.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "31.6.0",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"jest": "^26.5.2",
		"prettier": "^1.19.1",
		"ts-jest": "^26.4.1",
		"ts-node": "^8.10.2",
		"typedoc": "^0.19.2",
		"typedoc-plugin-markdown": "^3.0.8",
		"typescript": "^3.9.7"
	}
}
