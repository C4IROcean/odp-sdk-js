{
	"name": "@odp/sdk",
	"version": "0.3.2",
	"description": "Javascript client library for ODP SDK, built on top of Cognite SDK",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run format-code && npm run tslint && tsc && npm test",
		"build:ci": "npm run tslint && tsc --sourceMap false",
		"docs": "typedoc --options typedoc.js --tsconfig tsconfig.json",
		"tslint": "tslint -e \"**/*.d.ts\" -e \"node_modules/**/*ts\" \"**/*.ts\"",
		"format-code": "prettier package.json index.ts \"source/**/*.ts\" --write",
		"test": "jest"
	},
	"keywords": [
		"ODP",
		"sdk",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "git@ssh.dev.azure.com:v3/oceandatafoundation/ODP/ODP-Cognite"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@cognite/sdk": "^2.22.0",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/jest": "^24.9.1",
		"@types/node": "^13.7.7",
		"jest": "^24.9.0",
		"prettier": "^1.19.1",
		"ts-jest": "^24.2.0",
		"ts-node": "^8.5.2",
		"tslint": "^5.20.1",
		"typescript": "^3.7.2",
		"typedoc": "^0.13.0",
		"typedoc-plugin-markdown": "^1.1.19"
	}
}
